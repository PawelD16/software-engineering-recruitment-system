@model IEnumerable<projektowaniaOprogramowania.ViewModels.CollegeStructures.KierunekViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NazwaKierunku)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SkrotKierunku)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfilKierunku)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Czesne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CzesneDlaCudzoziemcow)
