            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dyscyplina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jezyk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StopienStudiow)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrybStudiowania)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Wydzial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Przelicznik)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NazwaKierunku)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkrotKierunku)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfilKierunku)
