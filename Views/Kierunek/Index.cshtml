                @Html.DisplayFor(modelItem => item.Czesne)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CzesneDlaCudzoziemcow)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CzasTrwaniaWSemestrach)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dyscyplina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jezyk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StopienStudiow)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrybStudiowania)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wydzial.NazwaWydzialu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Przelicznik.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
